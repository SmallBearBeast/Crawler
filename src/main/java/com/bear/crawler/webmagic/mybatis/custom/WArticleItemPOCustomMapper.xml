<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bear.crawler.webmagic.mybatis.custom.WArticleItemPOCustomMapper">
    <!-- 复用WArticleItemPOMapper里面的BaseResultMap -->
    <resultMap id="BaseResultMap" type="com.bear.crawler.webmagic.mybatis.generator.po.WArticleItemPO"
               extends="com.bear.crawler.webmagic.mybatis.generator.mapper.WArticleItemPOMapper.BaseResultMap"/>

    <select id="selectLatest" resultMap="BaseResultMap">
        select * from w_article_item where update_time >= (
            select max(update_time) from w_article_item where official_account_fake_id = #{fakeId}
        )
    </select>

    <select id="selectLatest_1" resultType="java.lang.String">
        select max(update_time) from w_article_item where official_account_fake_id = #{fakeId}
    </select>
</mapper>